[% USE Branches %]
[% PROCESS 'member-main-address-style.inc' %]
[% PROCESS 'member-alt-address-style.inc' %]
[% PROCESS 'member-alt-contact-style.inc' %]
[% PROCESS 'restriction-types.inc' %]
[% PROCESS 'i18n.inc' %]
[% SET footerjs = 1 %]
[% PROCESS 'patron-search.inc' %]
[% INCLUDE 'doc-head-open.inc' %]
<title>[% FILTER collapse %]
 [% UNLESS blocking_error %]
 [% IF ( op == 'add_form' ) %]
 [% t("Add patron") | html %]
 [% ELSE %]
 [% t("Modify patron") | html %]
 [% END %]
 [% INCLUDE 'patron-title.inc' no_html = 1 %]
 [% IF patron_category %]([% patron_category.description | html %])[% END %]
 [% END %] &rsaquo;
 [% t("Patrons") | html %] &rsaquo;
 [% t("Koha") | html %]
[% END %]</title>
[% INCLUDE 'doc-head-close.inc' %]
<style>[% FILTER collapse %]
fieldset legend {
    border: 1px solid #FFF;
    margin-left: -.5em;
    margin-top: -.5em;
    padding: .7em;
}
fieldset legend:hover {
    border: 1px solid #6FAF44;
    cursor: pointer;
}

fieldset legend i {
    color: #4C7AA8;
    font-size: 80%;
    padding-right: .2rem;
}

fieldset legend.collapsed {
    margin-bottom: -.5em;
}

legend.collapsed i.fa.fa-caret-down::before {
    content: "\f0da";
}
[% END %]</style>
</head>

<body id="pat_memberentrygen" class="pat">
 [% WRAPPER 'header.inc' %]
 [% INCLUDE 'patron-search-header.inc' %]
 [% END %]

 [% WRAPPER 'sub-header.inc' %]
 [% WRAPPER breadcrumbs %]
 [% WRAPPER breadcrumb_item %]
 <a href="/cgi-bin/koha/members/members-home.pl">المستفيدين</a>
 [% END %]
 [% UNLESS blocking_error %]
 [% UNLESS op == 'add_form' %]
 [% IF (borrower_data.firstname || borrower_data.surname ) %]
 [% WRAPPER breadcrumb_item %]
 <a href="/cgi-bin/koha/members/moremember.pl?borrowernumber=[% borrowernumber | uri %]">
 [% INCLUDE 'patron-title.inc' %]
 </a>
 [% END %]
 [% END %]
 [% END %]
 [% END %]
 [% WRAPPER breadcrumb_item bc_active= 1 %]
 [% IF ( op == 'add_form' ) %]
 <span>إضافة مستفيد</span>
 [% ELSE %]
 <span>تعديل المستفيد</span>
 [% END %]
 [% IF patron_category %]
 ([% patron_category.description | html %])
 [% END %]
 [% END %]
 [% END #/ WRAPPER breadcrumbs %]
 [% END #/ WRAPPER sub-header.inc %]

 <div class="main container-fluid">
 <div class="row">
 [% IF borrower_data.messages %]
 [% FOR message IN borrower_data.messages %]
 [% SWITCH message.error %]
 [% CASE 'error_on_insert_patron' %]
 <div class="dialog alert">لقد حدث خطأ ما أثناء إنشاء المستفيد. تحقق من السجلات للتفاصيل.</div>
 [% CASE 'error_on_update_patron' %]
 <div class="dialog alert">لقد حدث خطأ ما أثناء تحديث المستفيد. تحقق من السجلات للتفاصيل.</div>
 [% CASE %]<div class="dialog alert">خطأ غير معالج: [% message.error | html %]</div>
 [% END %]
 [% END %]
 [% END %]
 [% IF ( op == 'add_form' ) %]
 <div class="col-md-10 col-md-offset-1 col-lg-8 col-lg-offset-2">
 [% ELSE %]
 <div class="col-sm-10 col-sm-push-2">
 [% END %]
 <main class="clearfix">
 [% INCLUDE 'messages.inc' %]

 [% IF error_alert %]
 [% IF ( error_alert == "no_email" ) %]
 <div class="dialog alert">هذا العضو ليس لديه بريد إلكتروني</div>
 [% ELSE %]
 <div class="dialog alert">[% error_alert | html %]</div>
 [% END %]
 [% END %]
 [% IF info_alert %]
 <div class="dialog message">رسالة بريد الكتروني تمَّ إرسالها.</div>
 [% END %]

 [% INCLUDE 'noadd-warnings.inc' %]

 [% UNLESS ( no_add ) %]
 <h1>
 [% IF ( op == 'add_form' ) %]
 <span>إضافة مستفيد</span>
 [% ELSIF ( op == 'duplicate' ) %]
 <span>تكرار مستفيد</span>
 [% ELSE %]
 <span>تعديل المستفيد</span>
 [% END %]
 [% INCLUDE 'patron-title.inc' %]
 [% IF patron_category %]([% patron_category.description | html %])[% END %]
 </h1>

 [% IF quickadd && op == 'add_form' && !check_member %]
 <a href="#" class="toggle_quick_add"><i class="fa fa-plus-square"></i> إظهار النموذج الكامل</a>
 <a href="#" class="toggle_quick_add" style="display:none"><i class="fa fa-minus-square"></i> إظهار النموذج المختصر</a>
 [% END %]

 [% IF ( check_member ) %]
 <div class="dialog alert">
 <h3>تكرار سجل المستفيد؟</h3>
 <p><a class="popup_patronview" href="/cgi-bin/koha/members/moremember.pl?print=brief&amp;borrowernumber=[% check_member | uri %]"><i class="fa-solid fa-window-restore"></i> عرض تسجيلة موجودة</a></p>
 <button id="duplicate" type="submit" class="new"> <i class="fa-solid fa-pencil" aria-hidden="true"></i> هو تكرار. قم بتحرير التسجيلة الحالية </button>
 <button type="submit" id="not-duplicate" class="new"> <i class="fa fa-plus"></i> ليست تكرار. حفظ كتسجيلة جديدة </button>
 </div>
 [% END %]

 [% IF ( nok ) %]
 <div class="dialog alert">
 <p>الحقول التاليه خاطئة. يرجى إصلاحها.</p>
 <ul>
 [% IF ( ERROR_login_exist ) %]
 <li id="ERROR_login_exist">اسم المستخدم موجود بالفعل أو تعذر إنشاء اسم جديد فريد.</li>
 [% END %]
 [% IF ERROR_cardnumber_already_exists %]
 <li id="ERROR_cardnumber">رقم البطاقة قيد الاستخدام بالفعل.</li>
 [% END %]
 [% IF ERROR_cardnumber_length %]
 <li id="ERROR_cardnumber">طول رقم البطاقة غير صحيح.</li>
 [% END %]
 [% IF ( ERROR_age_limitations ) %]
 <li id="ERROR_age_limitations">عمر المستفيد غير صحيح بالنسبة لفئته. الأعمار المسموح بها هي: [% age_low | html %]-[% age_high | html %].</li>
 [% END %]
 [% IF ( ERROR_branch ) %]
 <li id="ERROR_branch">المكتبة غير صحيحة.</li>
 [% END %]
 [% IF ( ERROR_dateofbirth ) %]
 <li id="ERROR_dateofbirth">تاريخ الميلاد غير صالح.</li>
 [% END %]
 [% IF ( ERROR_dateenrolled ) %]
 <li id="ERROR_dateenrolled">تاريخ الاشتراك غير صالح.</li>
 [% END %]
 [% IF ( ERROR_dateexpiry ) %]
 <li id="ERROR_dateexpiry">تاريخ الانتهاء غير صالح.</li>
 [% END %]
 [% IF ( ERROR_password_too_short ) %]
 <li id="ERROR_short_password">كلمة المرور يجب أن تكون على الأقل [% minPasswordLength | html %] من طول الأحرف.</li>
 [% END %]
 [% IF ( ERROR_password_too_weak ) %]
 <li id="ERROR_weak_password">يجب أن تحتوي كلمة المرور على الأقل على رقم واحد، حرف صغير، وحرف كبير.</li>
 [% END %]
 [% IF ( ERROR_password_has_whitespaces ) %]
 <li id="ERROR_weak_password">لا يجب أن تحتوي كلمة المرور على مسافات سابقة أو لاحقة.</li>
 [% END %]
 [% IF ( ERROR_password_mismatch ) %]
 <li id="ERROR_password_mismatch">كلمتي المرور غير متطابقتين.</li>
 [% END %]
 [% IF ( ERROR_password_expiration_date ) %]
 <li id="ERROR_dateexpiry">تاريخ انتهاء كلمة المرور غير صالح.</li>
 [% END %]
 [% IF ( ERROR_extended_unique_id_failed ) %]
 <li id="ERROR_extended_unique_id_failed"><strong>[% ERROR_extended_unique_id_failed_description | html %]:</strong> قيمة الاستناد "[% ERROR_extended_unique_id_failed_value | html %]"قيد الاستخدام بالفعل بواسطة تسجيلة مستفيد أخرى.</li>
 [% END %]
 [% IF ERROR_bad_email %]
 <li id="ERROR_bad_email">البريد الالكتروني الرئيسي غير صالح.</li>
 [% END %]
 [% IF ERROR_bad_email_secondary %]
 <li id="ERROR_bad_email_secondary">البريد الإلكتروني الثانوي غير صالح.</li>
 [% END %]
 [% IF ERROR_bad_email_alternative %]
 <li id="ERROR_bad_email_alternative">البريد الإلكتروني البديل غير صحيح.</li>
 [% END %]
 [% IF ( ERROR_child_no_guarantor ) %]
 <li id="ERROR_child_no_guarantor">يحتاج المستفيد الطفل إلى ضامن.</li>
 [% END %]
 [% IF ( ERROR_guarantor_is_guarantee ) %]
 <li id="ERROR_guarantor_is_guarantee">لا يمكن أن يكون الضامن مكفولاً.</li>
 [% END %]
 [% IF ( ERROR_cannot_delete_guarantor ) %]
 <li id="ERROR_cannot_delete_guarantor">لا يمكن حذف الضامن. يحتاج المستفيد الطفل إلى ضامن.</li>
 [% END %]
 </ul>
 </div>
 [% END %]

 [% SET fieldstohide = Koha.Preference('CollapseFieldsPatronAddForm') %]
 [% IF Koha.Preference('CollapseFieldsPatronAddForm') %][% UNLESS step %]
 <p id="selections">
 <label><input id="toggle_hidden_fields" title="يتم طي هذه الحقول افتراضياً بواسطة التفضيل CollapseFieldsPatronAddForm" type="checkbox"><strong>عرض الحقول المطوية:</strong></label>
 [% FOREACH field IN fieldstohide.split(',') %]
 [% SWITCH field %]
 [% CASE 'identity' %] <span>هوية المستفيد</span> |
 [% CASE 'guarantor' %] <span>معلومات الضامن</span> |
 [% CASE 'nonpatron_guarantor' %] <span>كفيل غير مستفيد</span> |
 [% CASE 'primary_address' %] <span>العنوان الرئيسي</span> |
 [% CASE 'primary_contact' %] <span>معلومات الإتصال</span> |
 [% CASE 'alt_address' %] <span>عنوان بديل</span> |
 [% CASE 'alt_contact' %] <span>جهة اتصال أخرى</span> |
 [% CASE 'lib_mgmt' %] <span>إدارة المكتبة</span> |
 [% CASE 'lib_setup' %] <span>تنصيب المكتبة</span> |
 [% CASE 'login' %] <span>تسجيل دخول واجهة الموظف/الأوباك</span> |
 [% CASE 'flags' %] <span>إشارات حساب المستفيد</span> |
 [% CASE 'debarments' %] <span>قيود المستفيد</span> |
 [% CASE 'housebound' %][% IF Koha.Preference('HouseboundModule')%] <span>قواعد المرابطة بالمنزل</span> |[% END %]
 [% CASE 'additional' %] <span>معرفات وسمات إضافية</span> |
 [% CASE 'messaging' %] <span>تفضيلات مراسلات المستفيدين</span> |
 [% END %]
 [% END %]
 </p>
 [% END %][% END %]

 [% UNLESS ( check_member ) %]
 <div id="toolbar" class="btn-toolbar">
 [% IF quickadd && op == 'add_form' %]
 <button class="btn btn-primary toggler" id="save_quick_add" name="save"><i class="fa fa-save"></i> حفظ</button>
 [% END %]
 <button class="btn btn-primary toggler" id="saverecord" name="save" ><i class="fa fa-save"></i> حفظ</button>
 [% IF op == 'add_form' %]
 <a class="btn btn-link" href="/cgi-bin/koha/members/member.pl" class="toggler save_entryform">
 [% ELSE %]
 <a class="btn btn-link" href="/cgi-bin/koha/members/moremember.pl?borrowernumber=[% borrowernumber | html %]">
 [% END %]
 <i class="fa fa-times"></i> إلغاء </a>
 </div>
 [% END %]

 <form name="form" id="entryform" action="/cgi-bin/koha/members/memberentry.pl" method="post" autocomplete="off" class="toggler">
 [% UNLESS ( check_member ) %]
 <input type="hidden" name="nodouble"  value="[% nodouble | html %]" />
 [% END %]
 <!-- field always hidden in different form (1,2,3) -->
 <input type="hidden" name="BorrowerMandatoryField" value="[% BorrowerMandatoryField | html %]" />
 <input type="hidden" name="updtype" value="[% updtype | html %]" />
 <input type="hidden" name="destination" value="[% destination | html %]" />
 <input type="hidden" name="check_member" value="[% check_member | html %]" />
 <input type="hidden" name="borrowernumber" value="[% borrowernumber | html UNLESS op == 'duplicate' %]" />
 <input type="hidden" name="nodouble"  value="[% nodouble | html UNLESS op == 'duplicate' %]" />
 [% INCLUDE 'csrf-token.inc' %]
 [% IF ( step ) %]
 <input type="hidden" name="step"  value="[% step | html %]" />
 [% END %]
 [% IF ( op == 'add_form' ) %]
 <input type="hidden" name="op" value="cud-insert" />
 [% ELSIF ( op == 'duplicate' ) %]
 <input type="hidden" name="op" value="cud-insert" />
 [% ELSE %]
 <input type="hidden" name="op" value="cud-save" />
 [% IF step == 4 || step == 5 || step == 6 || step == 2 || step == 1 || step == 7 %]
 [%# Only put the card number if we arent showing it in the form later %]
 [% IF borrower_data.cardnumber %]
 <input type="hidden" name="cardnumber" value="[% borrower_data.cardnumber | html %]" />
 [% END %]
 [% END %]
 [% END %]

 [% IF ( step_1 ) %]
 [% UNLESS notitle && nosurname && nofirstname && nomiddle_name && nodateofbirth && noinitials && noothernames &&nosex && nopronouns %]
 <fieldset class="rows" id="memberentry_identity">
 <legend class="expanded" id="identity_lgd">
 <i class="fa fa-caret-down" title="طي هذا القسم"></i>
 [% IF ( patron_category.category_type == 'I' ) %]<span>المؤسسة [% ELSE %]المستفيد [% END %]الهوية</span>
 </legend>
 <ol>
 [% UNLESS ( patron_category.category_type == 'I' ) %]
 [% UNLESS notitle %]
 [% IF Koha.Preference('BorrowersTitles') %]
 <li>
 [% IF ( mandatorytitle ) %]
 <label for="btitle" class="required">
 [% ELSE %]
 <label for="btitle">
 [% END %] التحية: </label>
 <select id="btitle" name="title">
 <option value=""></option>
 [% FOREACH patron_title IN Koha.Preference('BorrowersTitles').split('\|') %]
 [% IF btitle == patron_title %]
 <option value="[% patron_title | html %]" selected="selected">[% patron_title | html %]</option>
 [% ELSE %]
 <option value="[% patron_title | html %]">[% patron_title | html %]</option>
 [% END %]
 [% END %]
 </select>
 [% IF ( mandatorytitle ) %]
 <span class="required">مطلوب</span>
 [% END %]
 </li>
 [% END # /IF Koha.Preference('BorrowersTitles') %]
 [% END # /UNLESS notitle %]
 [% END # /UNLESS ( I ) %]

 [% UNLESS nosurname %]
 <li>
 [% IF ( patron_category.category_type == 'I' ) %]
 <label for="surname" class="required">
 الاسم: </label>
 [% ELSE %]
 [% IF ( mandatorysurname ) %]
 <label for="surname" class="required">
 [% ELSE %]
 <label for="surname">
 [% END %] اللقب: </label>
 [% END %]

 [% IF ( uppercasesurnames ) %]
 <input style="text-transform:uppercase;" type="text" id="surname" name="surname" size="20"  value="[% patron.surname | html %]" />
 [% ELSE %]
 <input type="text" id="surname" name="surname" size="20"  value="[% patron.surname | html %]" />
 [% END %]
 [% IF ( mandatorysurname ) %]
 <span class="required">مطلوب</span>
 [% END %]
 </li>
 [% END # /UNLESS nosurname %]

 [% UNLESS ( patron_category.category_type == 'I' ) %]
 [% UNLESS nofirstname %]
 <li>
 [% IF ( mandatoryfirstname ) %]
 <label for="firstname" class="required">
 [% ELSE %]
 <label for="firstname">
 [% END %] الاسم الأول: </label>
 <input type="text" id="firstname" name="firstname" size="20"  value="[% borrower_data.firstname | html UNLESS op == 'duplicate' %]" />
 [% IF ( mandatoryfirstname ) %]
 <span class="required">مطلوب</span>
 [% END %]
 </li>
 [% END #/UNLESS nofirstname %]
 [% UNLESS nomiddle_name %]
 <li>
 [% IF ( mandatorymiddle_name ) %]
 <label for="middle_name" class="required">
 [% ELSE %]
 <label for="middle_name">
 [% END %] الاسم الأوسط: </label>
 <input type="text" id="middle_name" name="middle_name" size="20"  value="[% borrower_data.middle_name | html UNLESS op == 'duplicate' %]" />
 [% IF ( mandatorymiddle_name ) %]
 <span class="required">مطلوب</span>
 [% END %]
 </li>
 [% END #/UNLESS nomiddle_name %]
 [% UNLESS nodateofbirth %]
 <li>
 [% IF ( mandatorydateofbirth ) %]
 <label for="dateofbirth" class="required">
 [% ELSE %]
 <label for="dateofbirth">
 [% END %] تاريخ الميلاد: </label>
 <input type="text" id="dateofbirth" name="dateofbirth" size="20" value="[% borrower_data.dateofbirth | html UNLESS op == 'duplicate' %]" class="flatpickr" data-flatpickr-pastinclusive="true" />
 [% IF ( mandatorydateofbirth ) %]
 <span class="required">مطلوب</span>
 [% END %]
 [% IF ( ERROR_dateofbirth ) %]
 <span class="required">(خطأ)</span>
 [% END %]
 <div id="dateofbirth_hint" class="hint">[% INCLUDE 'date-format.inc' %]</div>
 </li>
 [% END # /UNLESS nodateofbirth %]
 [% UNLESS noinitials %]
 <li>
 [% IF ( mandatoryinitials ) %]
 <label for="initials" class="required">
 [% ELSE %]
 <label for="initials">
 [% END %] الحروف الأولى من الاسم: </label>
 <input type="text" id="initials" name="initials" size="20"  value="[% borrower_data.initials | html UNLESS op == 'duplicate' %]" />
 [% IF ( mandatoryinitials ) %]
 <span class="required">مطلوب</span>
 [% END %]
 </li>
 [% END # /UNLESS noinitials %]
 [% UNLESS nopronouns %]
 <li>
 [% IF ( mandatorypronouns ) %]
 <label for="pronouns" class="required">
 [% ELSE %]
 <label for="pronouns">
 [% END %] الضمائر الشخصية: </label>
 <input type="text" id="pronouns" name="pronouns" size="20"  value="[% borrower_data.pronouns | html UNLESS op == 'duplicate' %]" />
 [% IF ( mandatorypronouns ) %]
 <span class="required">مطلوب</span>
 [% END %]
 </li>
 [% END # /UNLESS nopronouns %]
 [% END #/UNLESS ( I ) %]
 [% UNLESS noothernames %]
 <li>
 [% IF ( mandatoryothernames ) %]
 <label for="othernames" class="required">
 [% ELSE %]
 <label for="othernames">
 [% END %] اسم آخر: </label>
 <input type="text" id="othernames" name="othernames" size="20"  value="[% borrower_data.othernames | html UNLESS op == 'duplicate' %]" />
 [% IF ( mandatoryothernames ) %]
 <span class="required">مطلوب</span>
 [% END %]
 [% IF ( patron_category.category_type == 'I' ) %]<input type="hidden" name="sex" value="N" />[% END %]
 </li>
 [% END #/UNLESS noothernames %]

 [% UNLESS ( patron_category.category_type == 'I' ) %]
 [% UNLESS nosex %]
 <li class="radio">
 [% UNLESS ( op == 'duplicate' ) %]
 [% IF ( female ) %]
 <label for="sex-female"><input type="radio" name="sex" id="sex-female" value="F" checked="checked" /><span class="patronsex-female"> أنثى</span></label>
 [% ELSE %]
 <label for="sex-female"><input type="radio" name="sex" id="sex-female" value="F" /><span class="patronsex-female"> أنثى</span></label>
 [% END %]
 [% IF ( male ) %]
 <label for="sex-male"><input type="radio" name="sex" id="sex-male" value="M" checked="checked" /><span class="patronsex-male"> ذكر</span></label>
 [% ELSE %]
 <label for="sex-male"><input type="radio" name="sex" id="sex-male" value="M" /><span class="patronsex-male"> ذكر</span></label>
 [% END %]
 [% IF ( other ) %]
 <label for="sex-other"><input type="radio" name="sex" id="sex-other" value="O" checked="checked" /><span class="patronsex-other"> [% tp('gender', 'Other') | html %]</span></label>
 [% ELSE %]
 <label for="sex-other"><input type="radio" name="sex" id="sex-other" value="O" /><span class="patronsex-other"> [% tp('gender', 'Other') | html %]</span></label>
 [% END %]
 [% IF ( none ) %]
 <label for="sex-none"><input type="radio" name="sex" id="sex-none" value=""  checked="checked" /><span class="patronsex-none"> غير محدد</span></label>
 [% ELSE %]
 <label for="sex-none"><input type="radio" name="sex" id="sex-none" value="" /><span class="patronsex-none"> غير محدد</span></label>
 [% END %]
 [% ELSE %]
 <label for="sex-female"><span class="patronsex-female">أنثى </span></label><input type="radio" name="sex" id="sex-female" value="F" />
 <label for="sex-male"><span class="patronsex-male">ذكر </span></label><input type="radio" name="sex" id="sex-male" value="M" />
 <label for="sex-other"><span class="patronsex-other">[% tp('gender', 'Other') | html %] </span></label><input type="radio" name="sex" id="sex-other" value="O" />
 <label for="sex-none"><span class="patronsex-none">لم يتم تحديد شيء </span></label><input type="radio" name="sex" id="sex-none" value="" checked="checked" />
 [% END # /UNLESS ( op == 'duplicate' )%]
 </li>
 [% END # /UNLESS nosex %]
 [% END # /UNLESS ( I ) %]
 </ol>
 </fieldset> <!-- /#memberentry_identity -->
 [% END # hide fieldset %]

 [% IF show_guarantor || guarantor %]
 [% SET possible_relationships = Koha.Preference('borrowerRelationship') %]
 <div id="memberentry_guarantor_anchor">
 <fieldset id="memberentry_guarantor" class="rows">
 <legend class="expanded" id="patron_guarantor_lgd">
 <i class="fa fa-caret-down" aria-hidden="true"></i>
 Patron guarantor
 </legend>

 <div id="guarantor_relationships">
 [% FOREACH r IN relationships %]
 <fieldset>
 <ol>
 [% IF patron_category.category_type == 'I' %]
 <li class="guarantor-details" data-borrowernumber="[% r.guarantor_id | $raw %]">
 <span class="label">المؤسسة:</span>
 [% INCLUDE 'patron-title.inc' patron=r.guarantor hide_patron_infos_if_needed=1 %]
 </li>
 <li>
 <span class="label">العلاقة:</span>
 <span>[% r.relationship | html %]</span>
 </li>
 [% ELSE %]
 <li class="guarantor-details" data-borrowernumber="[% r.guarantor_id | $raw %]">
 <span class="label">الكفيل:</span>
 [% INCLUDE 'patron-title.inc' patron=r.guarantor hide_patron_infos_if_needed=1 %]
 </li>
 <li>
 <span class="label">العلاقة:</span>
 <span>[% r.relationship | html %]</span>
 </li>

 <li>
 <label for="delete_guarantor-[% r.id | uri %]">إزالة: </label>
 <input type="checkbox" id="delete_guarantor-[% r.id | uri %]" name="delete_guarantor" value="[% r.id | html %]" />
 </li>
 [% END %]
 </ol>
 </fieldset>
 [% END # END relationships foreach %]
 [% IF guarantor && (!relationships) %]
 <fieldset class="guarantor">
 <ol>
 <li class="guarantor-details" data-borrowernumber="0">
 <span class="label">الكفيل:</span>
 <a href="/cgi-bin/koha/members/moremember.pl?borrowernumber=[% guarantor.borrowernumber | uri %]" target="blank">[% guarantor.firstname | html %] [% guarantor.surname | html %] ([% guarantor.cardnumber | html %])</a>
 <input type="hidden" class="new_guarantor_id" name="new_guarantor_id" value="[% guarantor.borrowernumber | html %]"/>
 </li>

 [% UNLESS norelationship %]
 <li>
 [% IF mandatoryrelationship %]
 <label for="guarantor_relationship" class="required">علاقة: </label>
 <select class="new_guarantor_relationship" name="new_guarantor_relationship" required="required">
 [% ELSE %]
 <label for="guarantor_relationship">علاقة: </label>
 <select class="new_guarantor_relationship" name="new_guarantor_relationship">
 [% END %]
 <option value=""></option>
 [% FOREACH pr IN possible_relationships.split('\|') %]
 [% IF pr != "" %]
 <option value="[% pr | html %]">[% pr | html %]</option>
 [% END %]
 [% END %]
 </select>
 [% UNLESS mandatoryrelationship %]
 <span class="required">مطلوب</span>
 [% END %]
 </li>
 [% ELSE %]
 <input type="hidden" name="new_guarantor_relationship" value="" />
 [% END %]

 <li>
 <label for="guarantor_cancel">&nbsp;</label>
 <span><a href="#" class="guarantor_cancel"><i class="fa fa-trash-can" aria-hidden="true"></i> إزالة</a></span>
 </li>
 </ol>
 </fieldset>
 [% END %]
 </div> <!-- #/guarantor_relationships -->

 <fieldset class="guarantor" id="guarantor_template">
 <ol>
 <li class="guarantor-details" data-borrowernumber="0">
 <span class="label">الكفيل:</span>
 <a class="new_guarantor_link" href="#" target="blank">
 <span class="new_guarantor_firstname_text"></span> <span class="new_guarantor_surname_text"></span>
 (<span class="new_guarantor_id_text"></span>)
 </a>
 <input type="hidden" class="new_guarantor_id" name="new_guarantor_id" value=""/>
 </li>

 [% UNLESS norelationship %]
 <li>
 [% IF mandatoryrelationship %]
 <label for="guarantor_relationship" class="required">علاقة: </label>
 <select class="new_guarantor_relationship" name="new_guarantor_relationship" required="required">
 [% ELSE %]
 <label for="guarantor_relationship">علاقة: </label>
 <select class="new_guarantor_relationship" name="new_guarantor_relationship">
 [% END %]
 <option value="" selected></option>
 [% FOREACH pr IN possible_relationships.split('\|') %]
 [% IF pr != "" %]
 <option value="[% pr | html %]">[% pr | html %]</option>
 [% END %]
 [% END %]
 </select>
 [% IF mandatoryrelationship %]
 <span class="required">مطلوب</span>
 [% END %]
 </li>
 [% ELSE %]
 <input type="hidden" name="new_guarantor_relationship" value="" />
 [% END %]

 <li>
 <label for="guarantor_cancel">&nbsp;</label>
 <span><a href="#" class="guarantor_cancel"><i class="fa fa-trash-can"></i> إزالة</a></span>
 </li>
 </ol>
 </fieldset>

 <ol>
 <input type="hidden" id="guarantor_id" value="" />
 <input name="guarantor_surname" id="guarantor_surname" type="hidden" />
 <input name="guarantor_firstname" id="guarantor_firstname" type="hidden" />

 <li>
 <a href="#patron_search_modal" class="btn btn-default" data-toggle="modal"><i class="fa fa-plus"></i> إضافة كفيل</a>
 </li>

 [% IF Koha.Preference('AllowStaffToSetCheckoutsVisibilityForGuarantor') %]
 <li>
 <label for="privacy_guarantor_checkouts">عرض الإعارات للضامنين:</label>
 <select name="privacy_guarantor_checkouts" id="privacy_guarantor_checkouts">
 [% IF borrower_data.privacy_guarantor_checkouts %]
 <option value="0">لا</option>
 <option value="1" selected>نعم</option>
 [% ELSE %]
 <option value="0" selected>لا</option>
 <option value="1">نعم</option>
 [% END %]
 </select>
 <div class="hint">السماح لكفيل هذا المستفيد برؤية إعاراته من الأوباك</div>
 </li>
 [% END %]
 [% IF Koha.Preference('AllowStaffToSetFinesVisibilityForGuarantor') %]
 <li>
 <label for="privacy_guarantor_fines">عرض الرسوم للضامنين:</label>
 <select name="privacy_guarantor_fines" id="privacy_guarantor_fines">
 [% IF borrower_data.privacy_guarantor_fines %]
 <option value="0">لا</option>
 <option value="1" selected>نعم</option>
 [% ELSE %]
 <option value="0" selected>لا</option>
 <option value="1">نعم</option>
 [% END %]
 </select>
 <div class="hint">السماح لضامني هذا المستفيد بمشاهدة رسوم هذا المستفيد من الأوباك</div>
 </li>
 [% END %]
 </ol>
 </fieldset> <!-- /#memberentry_guarantor -->
 </div> <!-- #/memberentry_guarantor_anchor -->

 [% UNLESS nocontactname && nocontactfirstname && norelationship %]
 <fieldset class="rows" id="non_patron_guarantor">
 <legend class="expanded" id="non_patron_guarantor_lgd">
 <i class="fa fa-caret-down" aria-hidden="true"></i>
 Non-patron guarantor
 </legend>
 <ol>
 [% UNLESS nocontactname %]
 <li>
 [% IF ( mandatorycontactname ) %]
 <label for="contactname" class="required">
