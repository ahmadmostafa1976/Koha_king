 [% SET autoMemberNum = Koha.Preference('autoMemberNum') %]
 <fieldset class="rows" id="memberentry_library_management">
 <legend class="expanded" id="library_management_lgd">
 <i class="fa fa-caret-down" aria-hidden="true"></i>
 Library management
 </legend>
 <ol>
 [% UNLESS nocardnumber %]
 <li>
 [% IF mandatorycardnumber %]
 <label for="cardnumber" class="required">
 [% ELSE %]
 <label for="cardnumber" class="validated">
 [% END %] رقم البطاقة: </label>

 <!-- NOTE: div.hint closing tag isn't on the same line -->
 [% IF minlength_cardnumber == maxlength_cardnumber %]
 <input type="text" id="cardnumber" name="cardnumber" size="20" value="[% borrower_data.cardnumber | html %]" minlength="[% minlength_cardnumber | html %]" maxlength="[% maxlength_cardnumber | html %]" />
 [% IF mandatorycardnumber %]
 <span class="required">مطلوب</span>
 [% END %]
 <span id="cn_max" class="required">لا يمكن أن يكون رقم البطاقة أكثر من [% maxlength_cardnumber | html %] حرفاً.</span>
 <div class="hint">يجب أن يكون رقم البطاقة [% minlength_cardnumber | html %] حرفًا بالضبط. [% ELSIF minlength_cardnumber && maxlength_cardnumber %] <input type="text" id="cardnumber" name="cardnumber" size="20" value="[% borrower_data.cardnumber | html %]" minlength="[% minlength_cardnumber | html %]" maxlength="[% maxlength_cardnumber | html %]" />
 [% IF mandatorycardnumber %]
 <span class="required">مطلوب</span>
 [% END %]
 <span id="cn_max" class="required">لا يمكن أن يكون رقم البطاقة أكثر من [% maxlength_cardnumber | html %] حرفاً.</span>
 <div class="hint">يجب أن يتراوح رقم البطاقة بين [% minlength_cardnumber | html %] و [% maxlength_cardnumber | html %] حرفًا. [% ELSIF maxlength_cardnumber %] <input type="text" id="cardnumber" name="cardnumber" size="20" value="[% borrower_data.cardnumber | html %]" maxlength="[% maxlength_cardnumber | html %]" />
 [% IF mandatorycardnumber %]
 <span class="required">مطلوب</span>
 [% END %]
 <span id="cn_max" class="required">لا يمكن أن يكون رقم البطاقة أكثر من [% maxlength_cardnumber | html %] حرفاً.</span>
 <div class="hint">يمكن أن يصل رقم البطاقة إلى [% maxlength_cardnumber | html %] حرفًا. [% ELSE %] <input type="text" id="cardnumber" name="cardnumber" size="20" value="[% borrower_data.cardnumber | html %]" />
 [% IF mandatorycardnumber %]
 <span class="required">مطلوب</span>
 [% END %]
 <div class="hint">لا يوجد حد أدنى أو أقصى لطول الأحرف. [% END %] [% IF autoMemberNum %] [% IF mandatorycardnumber %] <br/><span class="error">تم تعيين autoMemberNum إلى تفعيل، ولكن تم وضع علامة على رقم البطاقة باعتباره إلزاميًا في BorrowerMandatoryField: تم تعطيل الحساب التلقائي.</span>
 [% ELSE %]
 <br/>اتركه فارغاً للحساب التلقائي أثناء التسجيل [% END %] [% END %] </div><!--/div.hint -->
 </li>
 [% END # /UNLESS nocardnumber %]

 [% UNLESS nobranchcode %]
 <li>
 <label for="libraries" class="required">المكتبة:</label>
 <select name="branchcode" id="libraries">
 [% PROCESS options_for_libraries libraries => Branches.all( selected => userbranch, only_from_group => 1 ) %]
 </select>
 <span class="required">مطلوب</span>
 </li>
 [% END %]

 <li>
 <label for="categorycode_entry" class="required">فئة: </label>
 <select id="categorycode_entry" name="categorycode">
 [% FOREACH category_type IN patron_categories.keys %]
 [% SWITCH category_type %]
 [% CASE 'C' %]
 <optgroup label="طفل">
 [% CASE 'A' %]
 <optgroup label="بالغ">
 [% CASE 'S' %]
 <optgroup label="موظف">
 [% CASE 'I' %]
 <optgroup label="المؤسسة">
 [% CASE 'P' %]
 <optgroup label="مهني">
 [% CASE 'X' %]
 <optgroup label="إحصائي">
 [% CASE %]
 <optgroup label="غير معروف">
 [% END %]
 [% FOREACH category IN patron_categories.$category_type %]
 [% IF category.categorycode == patron_category.categorycode %]
 <option value="[% category.categorycode | html %]" selected="selected" data-pwd-length="[% category.effective_min_password_length | html %]" data-pwd-strong="[% category.effective_require_strong_password | html %]" data-typename="[% category_type | html %]">[% category.description | html %]</option>
 [% ELSE %]
 <option value="[% category.categorycode | html %]" data-pwd-length="[% category.effective_min_password_length | html %]" data-pwd-strong="[% category.effective_require_strong_password | html %]" data-typename="[% category_type | html %]">[% category.description | html %]</option>
 [% END %]
 [% END %]
 </optgroup>
 [% END %]
 </select>
 [% IF limited_category %]
 <span title="تقتصر الفئة الحالية للمستفيد ([% patron_category.description | html %]) على المكتبات الأخرى"><i class="fa fa-exclamation-triangle" aria-hidden="true"></i></span>
 [% END %]
 <span class="required">مطلوب</span>
 </li>

