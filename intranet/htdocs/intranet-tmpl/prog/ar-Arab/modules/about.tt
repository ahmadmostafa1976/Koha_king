[% USE raw %]
[% USE HtmlTags %]
[% USE Koha %]
[% USE Asset %]
[% USE KohaDates %]
[% PROCESS 'i18n.inc' %]
[% SET footerjs = 1 %]
[% INCLUDE 'doc-head-open.inc' %]
<title>[% FILTER collapse %]
 [% t("About Koha") | html %] &rsaquo;
 [% t("Koha") | html %]
[% END %]</title>
[% INCLUDE 'doc-head-close.inc' %]
</head>
<body id="about_about" class="about">
[% INCLUDE 'header.inc' %]
[% PROCESS 'about-team.inc' %]

[% WRAPPER 'sub-header.inc' %]
 [% WRAPPER breadcrumbs %]
 [% WRAPPER breadcrumb_item %]
 <a href="#" aria-current="page">حول كوها</a>
 [% END %]
 [% END %]
[% END %]

<div class="main container-fluid">
 <div class="row">
 <div class="col-md-12">
 [% INCLUDE 'messages.inc' %]

 <h1>حول كوها</h1>

 [% WRAPPER tabs id= "abouttabs" %]
 [% WRAPPER tabs_nav %]
 [% WRAPPER tab_item linktab = 1 tabname = "about" bt_active = tab == "about" %] <span>معلومات المخدم</span> [% END %]
 [% WRAPPER tab_item linktab = 1 tabname = "perl" bt_active = tab == "perl" %] <span>أنظمة بيرل الفرعية</span> [% END %]
 [% WRAPPER tab_item linktab = 1 tabname = "sysinfo" bt_active = tab == "sysinfo" %] <span>معلومات النظام</span> [% END %]
 [% WRAPPER tab_item linktab = 1 tabname = "team" bt_active = tab == "team" %] <span>فريق كوها</span> [% END %]
 [% WRAPPER tab_item linktab = 1 tabname = "licenses" bt_active = tab == "licenses" %] <span>التراخيص</span> [% END %]
 [% WRAPPER tab_item linktab = 1 tabname = "translations" bt_active = tab == "translations" %] <span>تراجم</span> [% END %]
 [% WRAPPER tab_item linktab = 1 tabname = "history" bt_active = tab == "history" %] <span>الخط الزمني</span> [% END %]
 [% WRAPPER tab_item linktab = 1 tabname = "dedications" bt_active = tab == "dedications" %] <span>الإهداءات</span> [% END %]
 [% END %]
 [% WRAPPER tab_panels %]
 [% SWITCH tab %]
 [% CASE 'about' %] [% PROCESS about_panel %]
 [% CASE 'perl' %] [% PROCESS perl_panel %]
 [% CASE 'sysinfo' %] [% PROCESS sysinfo_panel %]
 [% CASE 'team' %] [% PROCESS team_panel %]
 [% CASE 'licenses' %] [% PROCESS licenses_panel %]
 [% CASE 'translations' %] [% PROCESS translations_panel %]
 [% CASE 'history' %] [% PROCESS history_panel %]
 [% CASE 'dedications' %] [% PROCESS dedications_panel %]
 [% CASE %] [% PROCESS about_panel %]
 [% END %]
 [% END %]
 [% END # /WRAPPER abouttabs %]
 </div> <!-- /.col-md-12 -->
 </div> <!-- /.row -->
<!-- the main div is closed in intranet-bottom.inc -->
[% INCLUDE 'intranet-bottom.inc' %]

[% BLOCK about_panel %]
 [% WRAPPER tab_panel tabname= "about" bt_active = 1 %]
 <table>
 <caption>معلومات المخدم</caption>
 <tr>
 <th scope="row">إصدارة كوها: </th>
 <td>[% kohaVersion | html %][% IF kohaCodename %] <strong>[% kohaCodename | html %]</strong>[% END %]</td>
 </tr>
 <tr>
 <th scope="row">إصدار نظام التشغيل ('اسم المستخدم -a'): </th>
 <td>[% osVersion | html %]</td>
 </tr>
 <tr>
 <th scope="row">مترجم بيرل: </th>
 <td>[% perlPath | html %]</td>
 </tr>
 <tr>
 <th scope="row">إصدار بيرل: </th>
 <td>[% perlVersion | html %]</td>
 </tr>
 <tr>
 <th scope="row">Perl @INC: </th>
 <td>
 [% FOREACH perlIncPat IN perlIncPath %]
 [% perlIncPat.perlinc | html %] <br />
 [% END %]
 </td>
 </tr>
 <tr>
 <th scope="row">إصدارة MySQL: </th>
 <td>[% mysqlVersion | html %]</td>
 </tr>
 <tr>
 <th scope="row">نسخة أباتشي: </th>
 <td>[% apacheVersion | html %]</td>
 </tr>
 [% IF (is_psgi) %]
 <tr>
 <th scope="row">PSGI: </th>
 <td>[% psgi_server | html %]</td>
 </tr>
 [% END %]
 [% IF Koha.Preference('SearchEngine') == 'Elasticsearch' %]
 <tr>
 <th scope="row">Elasticsearch: </th>
 [% IF elasticsearch_fatal_config_error %]
 <td><span class="bg-warning">[% elasticsearch_fatal_config_error | html %]</span></td>
 [% ELSE %]
 [% IF elasticsearch_status.running %]
 <td class="bg-success">
 [% ELSE %]
 <td>
 [% END %]
 <span>الإصدار:</span>
 <span>[% elasticsearch_status.version | html %]</span>
 |
 <span>العقد:</span>
 <span>[% elasticsearch_status.nodes.join(' / ') | html %]</span>
 |
 <span>حالة:</span>
 [% IF elasticsearch_status.running %]
 <span>جاري التشغيل</span>
 |
 <span>الفهارس:</span>
 [% FOREACH index IN elasticsearch_status.indexes %]
 [% index.index_name | html %]
 (<span>العدد:</span> <em>[% index.index_count | html %]</em>)
 [% UNLESS loop.last %], [% END %]
 [% END %]
 [% ELSE %]
 <span class="bg-warning">متوقف عن التشغيل</span>
 [% END %]
 </td>
 [% END # /IF elasticsearch_fatal_config_error %]
 </tr>
 [% END %]
 <tr>
 <th scope="row">تخبأة الذاكرة: </th>
 [% IF memcached_running AND is_memcached_still_active AND where_is_memcached_config == 'config_only' %]
 <td class="bg-success">
 [% ELSE %]
 <td>
 [% END %]
 <span>خوادم:</span>
 [% IF memcached_servers %]
 <span>[% memcached_servers | html %]</span>
 [% ELSE %]
 <span>غير معرّف</span>
 [% END %] |
 <span>مسافة الاسم:</span>
 [% IF memcached_namespace %]
 <span>[% memcached_namespace | html %]</span>
 [% ELSE %]
 <span>غير معرّف</span>
 [% END %] |
 <span>حالة:</span>
 [% IF memcached_servers %]
 [% IF memcached_running and is_memcached_still_active %]
 <span class="bg-success">جاري التشغيل</span>.
 [% ELSE %]
 <span class="bg-warning">متوقف عن التشغيل</span>.
 [% IF is_psgi %]
 <span>تذكر أن التخبأة يجب أن تبدأ قبل Plack.</span>
 [% END %]
 [% END %]
 [% ELSE %]
 <span>غير معروف</span>
 [% END %] |
 <span>تهيئة قراءة من:</span>
 [% SWITCH where_is_memcached_config %]
 [% CASE 'config_only' %]
 <span class="bg-success">koha-conf.xml</span>
 [% CASE 'ENV_only' %]
 <span class="bg-warning">ENV</span> <span>لاحظ أن المكان الصحيح لتعريف التهيئة المخبأة هي ملف $KOHA_CONF الخاص بك</span>
 [% CASE 'both' %]
 <span class="bg-warning">ENV and koha-conf.xml</span> <span>لاحظ أن المكان المناسب لتعريف التهيئة المخبأة هو ملف $KOHA_CONF الخاص بك. لتجنب أي سوء فهم لا يجب عليك تصدير التهيئة المخبأة من ENV.</span>
 [% CASE # nowhere %]
 <span class="bg-warning">لامكان</span> <span>لاحظ أن المكان المناسب لتعريف التهيئة المخبأة هو ملف $KOHA_CONF الخاص بك. في الوقت الحالي لا يوجد لديك أي تهيئة تخبأة صالحة معرفة.</span>
 [% END %]
 [% IF effective_caching_method != 'Cache::Memcached::Fast::Safe' %]
 | <span>طريقة التخزين المؤقت الفعالة:</span> [% effective_caching_method | html %]
 [% END %]
 </td>
 </tr>
 <tr>
 <th scope="row">نسخة زيبرا: </th>
 <td>[% zebraVersion | html %]</td>
 </tr>
 <tr>
 <th scope="row">حالة زيبرا: </th>
 [% IF (errZebraConnection == 10000) %]
 <td class="bg-danger"><strong>يبدو أن خادم زيبرا غير متاح. هل بدأ؟</strong></td>
 [% ELSIF (errZebraConnection) %]
 <td class="bg-warning"><strong>رسالة خطأ من زيبرا: [% ( errZebraConnection ) | html %]</strong></td>
 [% ELSE %]
 <td class="bg-success">جاري التشغيل</td>
 [% END %]
 </tr>
 <tr>
 <th scope="row">وسيط الرسائل: </th>
 [% IF warnConnectBroker %]
 <td class="bg-warning"><strong>استخدام استقصاء SQL</strong></td>
 [% ELSE %]
 <td class="bg-success">اسنخدام RabbitMQ</td>
 [% END %]
 </tr>
 <tr>
 <th scope="row">التاريخ والوقت: </th>
 <td>[% current_date_and_time | $KohaDates  with_hours => 1 %]</td>
 </tr>
 <tr>
 [% timezone_config_class = (time_zone.config_invalid) ? 'bg-warning' : '' %]
 [% timezone_env_class    = (time_zone.env_invalid)    ? 'bg-warning' : '' %]
 <th scope="row">المنطقة الزمنية: </th>
 <td>
 <span>مستخدم:</span> <span>[% time_zone.actual | html %]</span>
 [% IF time_zone.actual_bad_tz_fallback %]
 <span>(هذه قيمة بديلة نتيجة لتهيئة خاطئة)</span>
 [% END %]
 |
 <span>تهيئة:</span>
 [% IF time_zone.config != '' %]
 <span class="[% timezone_config_class | html %]">[% time_zone.config | html %]</span>
 [% ELSE %]
 <span>غير معرّف</span>
 [% END %] |
 <span>البيئة (المنطقة الزمنية):</span>
 [% IF time_zone.environment != '' %]
 <span class="[% timezone_env_class | html %]">[% time_zone.environment | html %]</span>
 [% ELSE %]
 <span>غير معرّف</span>
 [% END %]
 </td>
 </tr>
 </table>
 [% END # tab=about %]
[% END %]

[% BLOCK perl_panel %]
 [% WRAPPER tab_panel tabname= "perl" bt_active = 1 %]
 <table style="cursor:pointer">
 <caption>أنظمة بيرل الفرعية</caption>
 [% FOREACH tabl IN table %]
 <tr>
 [% FOREACH ro IN tabl.row %]
 [% IF ( ro.current ) %]
 [% IF ( ro.require ) %]
 <th scope="row" style="font-weight:bold;" title="النظام الفرعي الحالي">
 [% ELSE %]
 <th scope="row" style="font-weight:normal" title="النظام الفرعي الحالي">
 [% END %]
 [% ELSIF ( ro.upgrade ) %]
 [% IF ( ro.require ) %]
 <th scope="row" style="background-color:#FFCB2F;font-weight:bold;" title="ترقية النظام الفرعي مطلوبة">
 [% ELSE %]
 <th scope="row" style="background-color:#FFCB2F;font-weight:normal" title="ترقية النظام الفرعي مطلوبة">
 [% END %]
 [% ELSE %]
 [% IF ( ro.name == '' ) %]
 <th>
 [% ELSIF ( ro.require ) %]
 <th scope="row" style="background-color:#FF8A8A;font-weight:bold;" title="الوحدة النمطية المفقودة مطلوبة">
 [% ELSE %]
 <th scope="row" style="background-color:#FF8A8A;font-weight:normal" title="النظام الفرعي الاختياري مفقود">
 [% END %]
 [% END # /IF ro.current %]
 [% IF ( ro.name ) %]
 [% ro.name | html %]
 <span style="font-weight:normal; font-size:smaller">
 ([% ro.reqversion | html %]
 [% IF ro.maxversion %] - [% ro.maxversion | html %][% END %]
 [% IF ro.excversion %][% FOR v IN ro.excversion %], ![% v | html %][% END %][% END %])
 </span>
 [% END %]
 </th>
 [% IF ( ro.name == '' ) %]
 <td>
 [% ELSIF ( ro.version ) %]
 <td>[% ro.version | html %]
 [% ELSE %]
 <td style="font-weight:bold">غير مثبت [% END %] </td>
 [% END # /FOREACH ro %]
 </tr>
 [% END # /FOREACH tabl %]
 </table>
 [% END # tab=perl %]
[% END %]

[% BLOCK sysinfo_panel %]
 [% IF Asset.css("css/staff-global.css") == "" || Asset.js("js/vue/dist/erm.js") == "" %]
 [% SET warnMissingCompiledFiles = 1 %]
 [% END %]
 [% WRAPPER tab_panel tabname= "sysinfo" bt_active = 1 %]
 [% IF weasyprint_missing || warnPrefRequireChoosingExistingAuthority || warnPrefEasyAnalyticalRecords || warnPrefAnonymousPatronOPACPrivacy || warnPrefAnonymousPatronAnonSuggestions || warnPrefAnonymousPatronOPACPrivacy_PatronDoesNotExist || warnPrefAnonymousPatronAnonSuggestions_PatronDoesNotExist || warnPrefKohaAdminEmailAddress || warnPrefOpacHiddenItems || warnPrefPatronSelfRegistrationDefaultCategory || invalid_yesno.count || warnNoActiveCurrency || warnIsRootUser || xml_config_warnings.size || warnI18nMissing || AutoSelfCheckPatronDoesNotHaveSelfCheckPerm || AutoSelfCheckPatronHasTooManyPerm || warnStatisticsFieldsError || warnNoTemplateCaching || warnILLConfiguration || has_ai_issues || oauth2_missing_deps || bad_yaml_prefs || warnRelationships || log4perl_errors || config_bcrypt_settings_no_set || warnHiddenBiblionumbers.size || warnConnectBroker || elasticsearch_has_missing || warnMissingCompiledFiles || warnDbRowFormat %]
 [% IF ( warnDbRowFormat ) %]
 <h2>تنسيق صف قاعدة البيانات غير صحيح</h2>
 <p>جداول قاعدة البيانات ذات تنسيق صف آخر غير 'DYNAMIC': [% warnDbRowFormat | html %]</p>
 <p>قد تواجه مشكلات في الترقية إلى الإصدارات الأحدث من كوها ما لم تقم بتحديث تنسيق الصف لجداول قاعدة البيانات الخاصة بك.</p>
 <p>لمعرفة كيفية تجنب هذه المشكلة انظر صفحة ويكي ذات الصلة: <a href="https://wiki.koha-community.org/wiki/Database_row_format">تنسيق صف قاعدة البيانات</a>
 </p>
 [% END %]

 [% IF (warnIsRootUser) %]
 <h2>تحذير بخصوص المستخدم الحالي</h2>
 <p>أنت مسجل الدخول كمستخدم إداري لقاعدة البيانات. لا ينصح بذلك لأن بعض أجزاء كوها لن تعمل كما هو متوقع عند استخدام هذا الحساب.</p>
 <p>يرجى تسجيل الدخول بدلا من ذلك باستخدام حساب موظف عادي. لإنشاء حساب موظف، قم بإنشاء مكتبة، فئة مستفيد "موظف" وإضافة مستفيد جديد. ثم امنح هذا المستفيد صلاحيات من "المزيد" في شريط الأدوات.</p>
 [% END # /IF warnIsRootUser %]

 [% IF has_ai_issues %]
 <h2>مشكلات البيانات</h2>
 <p>
 تواجه بعض جداولك مشكلات في قيم auto_increment الخاصة بها مما قد يؤدي إلى فقدان البيانات. </p>
 <p>
 <strong>لا يجب عليك تجاهل هذا التحذير.</strong>
 </p>
 <p>
 . المشكلة هي عدم احتفاظ InnoDB بـauto_increment عبر إعادة بدء خادم SQL (يتم ضبطها في الذاكرة فقط). لذا عند بدء الخادم يتم ضبط قيم auto_increment إلى max(table.id)+1. </p>
 <p>لمعرفة كيفية تجنب هذه المشكلة انظر صفحة ويكي ذات الصلة: <a href="https://wiki.koha-community.org/wiki/DBMS_auto_increment_fix">إصلاح الزيادة التلقائية لنظام إدارة قواعد البيانات</a>
 </p>

 <h3>تم العثور على مشكلات</h3>
 [% IF ai_patrons %]
 <h4>المستفيدين</h4>
 <p>المعرفات التالية موجودة في كل من الجدولين [% "borrowers" | $HtmlTags tag="strong" %]و [% "deletedborrowers" | $HtmlTags tag="strong" %]:</p>
 <p>
 [% FOR p IN ai_patrons %]
 [% p.borrowernumber | html %]
 [% UNLESS loop.last %], [% END %]
 [% END %]
 </p>
 [% END %]
 [% IF ai_biblios %]
 <h4>التسجيلات الببليوغرافية</h4>
 <p>المعرفات التالية موجودة في كل من الجدولين [% "biblio" | $HtmlTags tag="strong" %]و [% "deletedbiblio" | $HtmlTags tag="strong" %]:</p>
 <p>
 [% FOR b IN ai_biblios %]
 [% b.biblionumber | html %]
 [% UNLESS loop.last %], [% END %]
 [% END %]
 </p>
 [% END %]
 [% IF ai_biblioitems %]
 <h4>التسجيلات الببليوغرافية</h4>
 <p>المعرفات التالية موجودة في كل من الجدولين [% "biblio" | $HtmlTags tag="strong" %]و [% "deletedbiblioitems" | $HtmlTags tag="strong" %]:</p>
 <p>
 [% FOR b IN ai_biblioitems %]
 [% b.biblioitemnumber | html %]
 [% UNLESS loop.last %], [% END %]
 [% END %]
 </p>
 [% END %]
 [% IF ai_items %]
 <h4>مواد</h4>
 <p>المعرفات التالية موجودة في كل من الجدولين [% "items" | $HtmlTags tag="strong" %]و [% "deleteditems" | $HtmlTags tag="strong" %]:</p>
 <p>
 [% FOR i IN ai_items %]
 [% i.itemnumber | html %]
 [% UNLESS loop.last %], [% END %]
 [% END %]
 </p>
 [% END %]
 [% IF ai_checkouts %]
 <h4>الإعارات</h4>
 <p>المعرفات التالية موجودة في كل من الجدولين [% "issues" | $HtmlTags tag="strong" %]و [% "old_issues" | $HtmlTags tag="strong" %]:</p>
 <p>
 [% FOR c IN ai_checkouts %]
 [% c.issue_id | html %]
 [% UNLESS loop.last %], [% END %]
 [% END %]
 </p>
 [% END %]
 [% IF ai_holds %]
 <h4>حجوزات</h4>
 <p>المعرفات التالية موجودة في كل من الجدولين [% "reserves" | $HtmlTags tag="strong" %]و [% "old_reserves" | $HtmlTags tag="strong" %]:</p>
 <p>
 [% FOR h IN ai_holds %]
 [% h.reserve_id | html %]
 [% UNLESS loop.last %], [% END %]
 [% END %]
 </p>
 [% END %]
 <br/>
 [% END # /IF has_ai_issues %]

 [% IF warnRelationships %]
 <h2>مشكلات علاقات المستفيد</h2>
 [% IF bad_relationships_count %]
 <p>تحتوي قاعدة بيانات على أزواج من المكفولين/الكفلاء بدون علاقة معرفة. تم ضبطهم إلى القيمة '_bad_data' في أعمدة [% "borrowers.relationship" | $HtmlTags tag="strong" %] و/أو [% "borrower_relationships.relationship" | $HtmlTags tag="strong" %]. قم بإصلاحها يدويًا عن طريق إعادة إنشاء هذه العلاقات، أو اطلب من مدير النظام تصحيح القيم.</p>
 [% END # /IF bad_relationships_count %]

 [% IF wrong_relationships %]
 <p>تم استخدام القيم التالية لعلاقات المكفول/الكفيل، لكنها غير موجودة في التفضيل 'borrowerRelationship':</p>
 <ul>
 [% FOR rel IN wrong_relationships %]
 <li>[% rel.0 | html %]</li>
 [% END %]
 </ul>
 <p>إذا كانت العلاقة هي التي تريدها، فيرجى إضافتها إلى تفضيل النظام 'borrowerRelationship'، وإلا اجعل مسؤول النظام يقوم بتصحيح القيم في [% "borrowers.relationship" | $HtmlTags tag="strong" %] أو [% "borrower_relationships.relationship" | $HtmlTags tag="strong" %]في قاعدة البيانات.</p>
 [% END # /IF wrong_relationships %]
 [% END # /IF warnRelationships %]

 [% IF log4perl_errors %]
 <h2>نظام التسجيل لا يعمل بشكل صحيح</h2>
 [% FOR e IN log4perl_errors %]
 [% SWITCH e %]
 [% CASE 'missing_config_entry' %]<span>لا يوجد إدخال 'log4perl_conf' في ملف التهيئة.</span>
 [% CASE 'cannot_read_config_file' %]<span>لا يمكن فتح ملف التهيئة log4perl.</span>
 [% CASE 'logfile_not_writable' %]<span>أحد ملفات السجل المدرجة في ملف التهيئة غير قابل للكتابة.</span>
 [% CASE 'cannot_init_module' %]<span>لا يمكن بدء نظام Koha::Logger الفرعي بشكل صحيح (تحقق من ملف السجل)</span>
 [% CASE %]<span>خطأ غير معروف '[% e | html %]'.</span>
 [% END %]
 [% END # /FOR e %]
 [% END # /IF log4perl_errors %]

 [% IF warnConnectBroker %]
 <h2>من المستحيل الاتصال بوسيط الرسائل</h2>
 حدث خطأ أثناء محاولة الاتصال بوسيط الرسائل (RabbitMQ)، تحقق من ملفات سجل كوها. <br/>
 ربما لم يتم تثبيته وتهيئته بشكل صحيح؟ <br/>
 يرجى الاتصال بمدير النظام. [% END # /IF warnConnectBroker %] [% IF weasyprint_missing || warnPrefRequireChoosingExistingAuthority || warnPrefEasyAnalyticalRecords || warnPrefAnonymousPatronOPACPrivacy || warnPrefAnonymousPatronAnonSuggestions || warnPrefAnonymousPatronOPACPrivacy_PatronDoesNotExist || warnPrefAnonymousPatronAnonSuggestions_PatronDoesNotExist || warnPrefKohaAdminEmailAddress || warnPrefOpacHiddenItems || warnPrefPatronSelfRegistrationDefaultCategory || invalid_yesno.count || warnNoActiveCurrency || AutoSelfCheckPatronDoesNotHaveSelfCheckPerm || AutoSelfCheckPatronHasTooManyPerm || warnStatisticsFieldsError || warnNoTemplateCaching || warnILLConfiguration || warnXSLT || oauth2_missing_deps || bad_yaml_prefs || warnIssuingRules || config_bcrypt_settings_no_set || warnHiddenBiblionumbers.size || warnFastCataloging %] <h2>تنبيهات بخصوص تهيئة النظام</h2>
 <table>
 <caption>التفضيلات والضوابط</caption>
 [% IF (warnFastCataloging) %]
 <tr>
